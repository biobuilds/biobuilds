+    IppLZ77Flush flush = IppLZ77FullFlush;
+
+    if(s == Z_NULL || s->strm == Z_NULL) 
+        return Z_STREAM_ERROR;
+    strm = s->strm;
+
+    if (strm->next_out == Z_NULL || (strm->next_in == Z_NULL && strm->avail_in != 0))
+    {
+        ERR_RETURN(strm, Z_STREAM_ERROR);
+    }
+
+    if(s->deflate_stat == Z_NULL)
+        return Z_STREAM_ERROR;
