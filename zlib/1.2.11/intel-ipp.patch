+    int             local_lit_stat[LIT_TAB_SIZE];
+    int             local_dist_stat[DIST_TAB_SIZE];
+    const Ipp8u*    input_buffer;
+    uInt            input_len;
+    Ipp32u          input_index;
+    IppStatus       status;
+    int             *p_lit_stat, *p_dist_stat;
+    Ipp8u           *p_table_window;
+    Ipp32s          *p_table_hash;
+    z_streamp       strm;
